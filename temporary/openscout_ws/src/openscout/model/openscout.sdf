<?xml version="1.0" ?>
<sdf version="1.6">
    <!-- ******create the world****** --> 
    <world name="openscout">
        <physics name="1ms" type="ignored">
            <max_step_size>0.01</max_step_size>
            <real_time_factor>1</real_time_factor>
        </physics>

        <plugin 
            filename="gz-sim-physics-system"
            name="gz::sim::systems::Physics">
        </plugin>

        <plugin
            filename="gz-sim-user-commands-system"
            name="gz::sim::systems::UserCommands">
        </plugin>

        <plugin
            filename="gz-sim-scene-broadcaster-system"
            name="gz::sim::systems::SceneBroadcaster">
        </plugin>
        
        <!-- ******add light inside the world****** -->
        <light type="directional" name="sun">
            <cast_shadows>true</cast_shadows>
            <pose>0 0 10 0 0 0</pose>
            <diffuse>0.8 0.8 0.8 1</diffuse>
            <specular>0.2 0.2 0.2 1</specular>
            <attenuation>
                <range>1000</range>
                <constant>0.9</constant>
                <linear>0.01</linear>
                <quadratic>0.001</quadratic>
            </attenuation>
            <direction>-0.5 0.1 -0.9</direction>
        </light>
        <!-- ******add ground plane inside the world****** -->
        <model name="ground_plane">
            <static>true</static>
            <link name="link">
                <collision name="collision">
                    <geometry>
                        <plane>
                        <normal>0 0 1</normal>
                        </plane>
                    </geometry>
                </collision>
                <visual name="visual">
                    <geometry>
                        <plane>
                        <normal>0 0 1</normal>
                        <size>100 100</size>
                        </plane>
                    </geometry>
                    <material>
                        <ambient>0.9 0.9 0.9 1</ambient>
                        <diffuse>0.9 0.9 0.9 1</diffuse>
                        <specular>0.9 0.9 0.9 1</specular>
                    </material>
                </visual>
            </link>
        </model>
        
        
        <!-- ****** front chassis ****** -->
        <model name='open_scout' canonical_link='front_chassis'>
            <pose relative_to='world'>0 0 0.2 0 0 0</pose>
            <link name='front_chassis'>
                <pose relative_to='__model__'>0 0 0.056 0 0 0</pose>
                <inertial>
                    <mass>3.5</mass>
                    <inertia>
                        <ixx>0.0233</ixx>
                        <ixy>0</ixy>
                        <ixz>0</ixz>
                        <iyy>0.0379</iyy>
                        <iyz>0</iyz>
                        <izz>0.0379</izz>
                    </inertia>
                </inertial>
                <!-- ******create the visual of the chassis****** -->
                <visual name='visual'>
                    <geometry>
                      <mesh>
                        <uri>meshes/OpenScoutFront.stl</uri>
                        <scale>0.001 0.001 0.001</scale>
                      </mesh>
                    </geometry>
                    <material>
                      <ambient>1.0 1.0 0.0 1</ambient>  <!-- Yellow color (RGB: 1, 1, 0) -->
                      <diffuse>1.0 1.0 0.0 1</diffuse>  <!-- Yellow color (RGB: 1, 1, 0) -->
                      <specular>1.0 1.0 0.0 1</specular>  <!-- Yellow color (RGB: 1, 1, 0) -->
                    </material>
                </visual>
                <collision name='collision'>
                    <geometry>
                      <mesh>
                        <uri>meshes/OpenScoutFront.stl</uri>
                        <scale>0.001 0.001 0.001</scale>
                      </mesh>
                    </geometry>
                </collision>
            </link>
            
                    <!-- Back Chassis -->
        <link name='back_chassis'>
            <pose relative_to='front_chassis'>0 0 0 0 0 0</pose> <!-- Adjust pose if necessary -->
            <inertial>
                <mass>5.5</mass>
                  <inertia>
                    <ixx>0.03667</ixx>
                    <ixy>0.0</ixy>
                    <ixz>0.0</ixz>
                    <iyy>0.05958</iyy>
                    <iyz>0.0</iyz>
                    <izz>0.05958</izz>
                  </inertia>
            </inertial>
            <visual name='visual'>
                <geometry>
                    <mesh>
                        <uri>meshes/OpenScoutBack.stl</uri>
                        <scale>0.001 0.001 0.001</scale>
                    </mesh>
                </geometry>
                <material>
                    <ambient>0.0 0.0 1.0 1</ambient> <!-- Blue color -->
                    <diffuse>0.0 0.0 1.0 1</diffuse> <!-- Blue color -->
                    <specular>0.0 0.0 1.0 1</specular> <!-- Blue color -->
                </material>
            </visual>
            <collision name='collision'>
                <geometry>
                    <mesh>
                        <uri>meshes/OpenScoutBack.stl</uri>
                        <scale>0.001 0.001 0.001</scale>
                    </mesh>
                </geometry>
            </collision>
        </link>


                    <!-- Joint between Front and Back Chassis -->
          <joint name='lazy_susan_joint' type='revolute'>
              <parent>front_chassis</parent>
              <child>back_chassis</child>
              <pose relative_to='front_chassis'>0 0 0 0 0 0</pose> <!-- Adjust pose to match bearing position -->
              <axis>
                  <xyz>0 1 0</xyz> <!-- Rotation around the X-axis -->
              </axis>
          </joint>

<!-- WHEELS --><!-- WHEELS --><!-- WHEELS --><!-- WHEELS --><!-- WHEELS --><!-- WHEELS --><!-- WHEELS -->
    
           <!-- Front Right Wheel -->
            <link name='front_right_wheel'>
                <pose relative_to="front_chassis">-0.2 -0.3 0.01 0 1.5708 0</pose>
                <inertial>
                    <mass>0.150</mass>
                    <inertia>
                        <ixx>0.000361275</ixx>
                        <ixy>0</ixy>
                        <ixz>0</ixz>
                        <iyy>0.000361275</iyy>
                        <iyz>0</iyz>
                        <izz>0.000361275</izz>
                    </inertia>
                </inertial>
                <visual name='visual'>
                    <geometry>
                        <cylinder>
                            <radius>0.056</radius>
                            <length>0.05</length>
                        </cylinder>
                        <!--<mesh>
                            <uri>meshes/test_wheel.stl</uri>
                            <scale>0.001 0.001 0.001</scale>
                        </mesh>-->
                    </geometry>
                    <material>
                        <ambient>1.0 0.0 0.0 1</ambient>  
                        <diffuse>1.0 0.0 0.0 1</diffuse>  
                        <specular>1.0 0.0 0.0 1</specular> 
                    </material>
                </visual>
                <collision name='collision'>
                    <geometry>
                        <cylinder>
                            <radius>0.056</radius>
                            <length>0.05</length>
                        </cylinder>
                        <!--<mesh>
                            <uri>meshes/test_wheel.stl</uri>
                            <scale>0.001 0.001 0.001</scale>
                        </mesh>-->
                    </geometry>
                </collision>
            </link>

            
            <!-- Front Left Wheel -->
            <link name='front_left_wheel'>
                <pose relative_to="front_chassis">0.2 -0.3 0.01 0 1.5708 0</pose>
                <inertial>
                    <mass>0.150</mass>
                    <inertia>
                        <ixx>0.000361275</ixx>
                        <ixy>0</ixy>
                        <ixz>0</ixz>
                        <iyy>0.000361275</iyy>
                        <iyz>0</iyz>
                        <izz>0.000361275</izz>
                    </inertia>
                </inertial>
                <visual name='visual'>
                    <geometry>
                        <cylinder>
                            <radius>0.056</radius>
                            <length>0.05</length>
                        </cylinder>
                        <!--<mesh>
                            <uri>meshes/test_wheel.stl</uri>
                            <scale>0.001 0.001 0.001</scale>
                        </mesh>-->
                    </geometry>
                    <material>
                        <ambient>1.0 0.0 0.0 1</ambient>  
                        <diffuse>1.0 0.0 0.0 1</diffuse>  
                        <specular>1.0 0.0 0.0 1</specular> 
                    </material>
                </visual>
                <collision name='collision'>
                    <geometry>
                        <cylinder>
                            <radius>0.056</radius>
                            <length>0.05</length>
                        </cylinder>
                        <!--<mesh>
                            <uri>meshes/test_wheel.stl</uri>
                            <scale>0.001 0.001 0.001</scale>
                        </mesh>-->
                    </geometry>
                </collision>
            </link>

            <!-- Back Right Wheel -->
            <link name='back_right_wheel'>
                <pose relative_to="back_chassis">-0.2 0.3 0.01 0 1.5708 0</pose>
                <inertial>
                    <mass>0.150</mass>
                    <inertia>
                        <ixx>0.000361275</ixx>
                        <ixy>0</ixy>
                        <ixz>0</ixz>
                        <iyy>0.000361275</iyy>
                        <iyz>0</iyz>
                        <izz>0.000361275</izz>
                    </inertia>
                </inertial>
                <visual name='visual'>
                    <geometry>
                        <cylinder>
                            <radius>0.056</radius>
                            <length>0.05</length>
                        </cylinder>
                        <!--<mesh>
                            <uri>meshes/test_wheel.stl</uri>
                            <scale>0.001 0.001 0.001</scale>
                        </mesh>-->
                    </geometry>
                    <material>
                        <ambient>1.0 0.0 0.0 1</ambient>  
                        <diffuse>1.0 0.0 0.0 1</diffuse>  
                        <specular>1.0 0.0 0.0 1</specular> 
                    </material>
                </visual>
                <collision name='collision'>
                    <geometry>
                        <cylinder>
                            <radius>0.056</radius>
                            <length>0.05</length>
                        </cylinder>
                        <!--<mesh>
                            <uri>meshes/test_wheel.stl</uri>
                            <scale>0.001 0.001 0.001</scale>
                        </mesh>-->
                    </geometry>
                </collision>
            </link>

            <!-- Back Left Wheel -->
            <link name='back_left_wheel'>
                <pose relative_to="back_chassis">0.2 0.3 0.01 0 1.5708 0</pose>
                <inertial>
                    <mass>0.150</mass>
                    <inertia>
                        <ixx>0.000361275</ixx>
                        <ixy>0</ixy>
                        <ixz>0</ixz>
                        <iyy>0.000361275</iyy>
                        <iyz>0</iyz>
                        <izz>0.000361275</izz>
                    </inertia>
                </inertial>
                <visual name='visual'>
                    <geometry>
                        <cylinder>
                            <radius>0.056</radius>
                            <length>0.05</length>
                        </cylinder>
                        <!--<mesh>
                            <uri>meshes/test_wheel.stl</uri>
                            <scale>0.001 0.001 0.001</scale>
                        </mesh>-->
                    </geometry>
                    <material>
                        <ambient>1.0 0.0 0.0 1</ambient>  
                        <diffuse>1.0 0.0 0.0 1</diffuse>  
                        <specular>1.0 0.0 0.0 1</specular> 
                    </material>
                </visual>
                <collision name='collision'>
                    <geometry>
                        <cylinder>
                            <radius>0.056</radius>
                            <length>0.05</length>
                        </cylinder>
                        <!--<mesh>
                            <uri>meshes/test_wheel.stl</uri>
                            <scale>0.001 0.001 0.001</scale>
                        </mesh>-->
                    </geometry>
                </collision>
            </link>

            <!-- Joints for all wheels -->
            <joint name="front_right_wheel_joint" type="revolute">
                <pose>0 0 0 0 0 0</pose>
                <parent>front_chassis</parent>
                <child>front_right_wheel</child>
                <axis>
                    <xyz expressed_in='__model__'>1 0 0</xyz>
                    <limit>
                        <lower>-1.79769e+308</lower>
                        <upper>1.79769e+308</upper>
                    </limit>
                </axis>
            </joint>
            
            <joint name="front_left_wheel_joint" type="revolute">
                <pose>0 0 0 0 0 0</pose>
                <parent>front_chassis</parent>
                <child>front_left_wheel</child>
                <axis>
                    <xyz expressed_in='__model__'>1 0 0</xyz>
                    <limit>
                        <lower>-1.79769e+308</lower>
                        <upper>1.79769e+308</upper>
                    </limit>
                </axis>
            </joint>

            <joint name="back_right_wheel_joint" type="revolute">
                <pose>0 0 0 0 0 0</pose>
                <parent>back_chassis</parent>
                <child>back_right_wheel</child>
                <axis>
                    <xyz expressed_in='__model__'>1 0 0</xyz>
                    <limit>
                        <lower>-1.79769e+308</lower>
                        <upper>1.79769e+308</upper>
                    </limit>
                </axis>
            </joint>
            
            <joint name="back_left_wheel_joint" type="revolute">
                <pose>0 0 0 0 0 0</pose>
                <parent>back_chassis</parent>
                <child>back_left_wheel</child>
                <axis>
                    <xyz expressed_in='__model__'>1 0 0</xyz>
                    <limit>
                        <lower>-1.79769e+308</lower>
                        <upper>1.79769e+308</upper>
                    </limit>
                </axis>
            </joint>


            <!--diff drive plugin-->
            <plugin
                filename="gz-sim-diff-drive-system"
                name="gz::sim::systems::DiffDrive">
                <left_joint>front_left_wheel_joint</left_joint>
                <left_joint>back_left_wheel_joint</left_joint>
                <right_joint>front_right_wheel_joint</right_joint>
                <right_joint>back_right_wheel_joint</right_joint>

                <!--<num_wheel_pairs>2</num_wheel_pairs>-->
                <wheel_separation>0.302</wheel_separation> <!-- Updated wheel separation to 302 mm (0.302 meters) -->
                <!--<wheelbase>0.395</wheelbase>-->
                <wheel_radius>0.056</wheel_radius>        <!-- Radius of the wheels (112mm diameter) -->
                
                <!-- new stuff-->
                <max_linear_acceleration>1</max_linear_acceleration>
                <min_linear_acceleration>-1</min_linear_acceleration>
                <max_angular_acceleration>2</max_angular_acceleration>
                <min_angular_acceleration>-2</min_angular_acceleration>
                <max_linear_velocity>0.5</max_linear_velocity>
                <min_linear_velocity>-0.5</min_linear_velocity>
                <max_angular_velocity>1</max_angular_velocity>
                <min_angular_velocity>-1</min_angular_velocity>
                <!-- new stuff end-->

                <update_rate>50</update_rate>
                <odom_publish_frequency>1</odom_publish_frequency> <!-- Odometry publishing frequency -->
                
                <topic>cmd_vel</topic> <!-- ROS topic for velocity commands -->
                <odom_topic>odom</odom_topic>
            </plugin>

            <plugin filename="gz-sim-joint-state-publisher-system" name="gz::sim::systems::JointStatePublisher">
                <topic>joint_states</topic>
                <joint_name>front_left_wheel_joint</joint_name>
                <joint_name>back_left_wheel_joint</joint_name>
                <joint_name>front_right_wheel_joint</joint_name>
                <joint_name>back_right_wheel_joint</joint_name>
            </plugin>

        </model>

    </world>
</sdf>